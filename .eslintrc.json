{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "eqeqeq": ["error", "always", {"null": "ignore"}],
        "indent": [ "warn", "tab" ],
        "linebreak-style": [ "warn", "unix" ],
        "quotes": [ "warn", "double" ],
        "semi": [ "warn", "always" ]
    }
}
